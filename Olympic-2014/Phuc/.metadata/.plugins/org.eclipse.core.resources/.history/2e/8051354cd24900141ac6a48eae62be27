		char s[] = (new Scanner(System.in)).next().toCharArray();
		List<Integer> list = new ArrayList<Integer>();
		int l = s.length;
		int c = 0;
		for (int i = 0; i < l; ++i) {
			if (s[i] == '7') {
				count++;
			} else {
