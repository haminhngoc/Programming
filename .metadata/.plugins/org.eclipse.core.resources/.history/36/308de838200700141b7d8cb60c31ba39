import java.sql.Ref;
import java.util.Scanner;

public class codeforces_444B_LovesFFT {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		int d = in.nextInt();
		long x = in.nextLong();
		solve(n, d, x);

	}

	private static void solve(int n, int d, long x) {

		long[] a = new long[n];
		int[] b = new int[0];
		for (int i = 0; i < n; i++) {
			a[i] = i + 1;
		}
		int index;
		long t;
		for (int i = 0; i < n; i++) {
			x = (x * 37 + 10007) % 1000000007;
			index = (int) (x % (i + 1));
			t = a[i];
			a[i] = a[index];
			a[index] = t;
		}

		for (int i = 0; i < n; i++) {
			b[i] = (i < d)?1:0;
		}

		for (int i = 0; i < n; i++) {
			x = (x * 37 + 10007) % 1000000007;
			index = (int) (x % (i + 1));
			t = a[i];
			a[i] = a[index];
			a[index] = t;
		}
		
		long max = -1;
		for(int i = 0; i <n; ++i){
			long current = a[i] *b[n-i]
		}
	}

}
