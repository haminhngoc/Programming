import java.util.Scanner;

public class Chemistry {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		int m = in.nextInt();
		int[][] arr = new int[n][m];
		for (int i = 0; i < n; ++i) {
			for (int j = 0; j < m; ++j) {
				arr[i][j] = 0;
			}
		}
		int a, b;
		for (int i = 0; i < m; ++i) {
			a = in.nextInt();
			b = in.nextInt();
			arr[a - 1][b - 1] = 1;
			arr[b - 1][a - 1] = 1;
		}
		solve(arr, n, m);

	}

	private static void solve(int[][] arr, int n, int m) {
		int[] group = new int[n];
		for (int i : group) {
			i = 0;
		}
		int currentGroup = 0;
		for (int i = 0; i < n; ++i) {
			DFS(arr, n, currentGroup, i, group);
			currentGroup++;
		}
	}

	private static void DFS(int[][] arr, int n, int currentGroup, int i,
			int[] group) {
		if (group[i] == 0) {
			group[i] = currentGroup;
			for (int j = i + 1; j < n; ++j) {
				DFS(arr, n, currentGroup, j, group);
			}
		}
	}

}
